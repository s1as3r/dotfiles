{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#98C379",
          "style": "plain",
          "template": "λ",
          "type": "text"
        },
        {
          "foreground": "#56B6C2",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#92C7CF",
          "properties": {
            "branch_icon": "",
            "fetch_status": true
          },
          "style": "plain",
          "template": "[{{ .HEAD }}{{ if .Staging.Changed }}<#00AA00> ● {{ .Staging.String }}</>{{ end }}{{ if .Working.Changed }}<#D75F00> ● {{ .Working.String }}</>{{ end }}]",
          "type": "git"
        },
        {
          "foreground": "#BF616A",
          "style": "plain",
          "template": " ✗",
          "type": "status"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "type": "text",
          "foreground": "#282C34",
          "template": "<#282C34>[{{ .Env.SHELL_VI_MODE }}]</>",
          "style": "plain"
        },
        {
          "foreground": "#282C34",
          "properties": {
            "time_format": "15:04"
          },
          "style": "plain",
          "template": "<#282C34>[{{ .CurrentDate | date .Format }}]</>",
          "type": "time"
        }
      ],
      "type": "rprompt"
    }
  ],
  "final_space": true,
  "version": 2
}
