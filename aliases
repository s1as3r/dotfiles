alias cls='clear '
alias py=python3
alias python=python3
alias ipy=ipython
alias duh='du -ha --max-depth=1'
alias rmfind='f(){ find . "$1" -exec rm {} \; ; unset -f f;}; f'
alias mcd='f(){ mkdir "$1" && cd "$1"; unset -f f;}; f'
alias gitsc='git clone --depth=1'
alias chx='chmod u+x'
alias c=clear
alias v=nvim
alias kali="sudo ~/VMs/Kali/Kali-Linux-2021.2-vmware-amd64.vmx"
alias msable="sudo ~/VMs/Metasploitable2/Metasploitable.vmx"

alias icat='kitty +kitten icat'

